{
	"name": "phaser-parcel-template",
	"version": "1.0.0",
	"description": "Template running Phaser 3 and Parcel 2",
	"scripts": {
		"start": "parcel src/index.html --open",
		"build": "rm -rf ./dist && parcel build src/index.html --no-source-maps --public-url ."
	},
	"author": "philkrause",
	"dependencies": {
		"phaser": "^3.55.2"
	},
	"devDependencies": {
		"parcel": "^2.4.1",
		"parcel-reporter-static-files-copy": "^1.3.4",
		"process": "^0.11.10"
	},
	"staticFiles": {
		"staticPath": "src/assets",
		"staticOutPath": "assets"
	}
}
